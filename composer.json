{
    "name": "epmnzava/bill-me",
    "description": "Complete Billing Package  Starter",
    "keywords": [
        "epmnzava",
        "bill-me"
    ],
    "homepage": "https://github.com/epmnzava/bill-me",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Emmanuel Mnzava",
            "email": "epmnzava@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Epmnzava\\BillMe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Epmnzava\\BillMe\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Epmnzava\\BillMe\\BillMeServiceProvider"
            ],
            "aliases": {
                "BillMe": "Epmnzava\\BillMe\\BillMeFacade"
            }
        }
    }
}
